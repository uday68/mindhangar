# Environment Configuration
NODE_ENV=development

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_secret_key_here_generate_with_openssl_rand_base64_32

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Database (Supabase PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/database
DIRECT_URL=postgresql://user:password@host:5432/database

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Security
JWT_SIGNING_KEY=your_jwt_signing_key_here
ENCRYPTION_KEY=your_encryption_key_here_32_bytes

# Optional: Monitoring and Error Tracking
SENTRY_DSN=
SENTRY_AUTH_TOKEN=

# Optional: Redis for session storage
REDIS_URL=
